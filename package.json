{
  "name": "ff",
  "version": "1.0.0",
  "description": "CLI used to run your forge in the file forge ecosystem",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc && tsc-alias",
    "watch": "nodemon --watch src --ext * --exec npm run build"
  },
  "keywords": ["cli", "file", "forge", "tooling", "template", "scaffolding"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bh-schmidt/file-forge-cli"
  },
  "bugs": {
    "url": "https://github.com/bh-schmidt/file-forge-cli/issues"
  },
  "homepage": "https://github.com/bh-schmidt/file-forge-cli#readme",
  "license": "MIT",
  "type": "module",
  "bin": {
    "ff": "./dist/index.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.17",
    "@types/prompts": "^2.4.9",
    "@types/proper-lockfile": "^4.1.4",
    "nodemon": "^3.1.9",
    "tsc-alias": "^1.8.13",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "fly-import": "^0.4.1",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.1",
    "lodash": "^4.17.21",
    "prompts": "^2.4.2",
    "proper-lockfile": "^4.1.2"
  }
}